{
  "name": "react-transmit-native",
  "description": "Relay-inspired library based on Promises instead of GraphQL.",
  "version": "3.1.3",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/RickWong/react-transmit.git#master-native"
  },
  "homepage": "https://github.com/RickWong/react-transmit/tree/master-native",
  "keywords": [
    "react",
    "transmit",
    "relay",
    "react-component",
    "native",
    "react-native",
    "ios"
  ],
  "main": "src/lib/react-transmit.js",
  "scripts": {
    "localhost": "sleep 3; which open && open http://localhost:8080",
    "build": "webpack --verbose --colors --display-error-details --config webpack.client.js",
    "watch-client": "webpack --verbose --colors --display-error-details --config webpack.client-watch.js && webpack-dev-server --config webpack.client-watch.js",
    "watch": "concurrent --kill-others 'npm run watch-client' 'npm run localhost'"
  },
  "dependencies": {
    "react-native": ">= 0.3.1",
    "ascii-json": "0.2.0"
  },
  "devDependencies": {
    "babel": "6.1.18",
    "babel-core": "6.2.1",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.1.18",
    "concurrently": "1.0.0",
    "fetch-rest": "1.0.0",
    "isomorphic-fetch": "2.2.0",
    "json-loader": "0.5.4",
    "react-hot-loader": "1.3.0",
    "react-inline-css": "2.0.1",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0"
  },
  "engines": {
    "node" : ">=0.10.32"
  }
}
